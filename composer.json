{
    "name" : "eskater/laravel-fcm",
    "description" : "A package to send Firebase notification cross Laravel Application",
    "authors": [
        {
            "name": "Muhammad Amirul Ihsan",
            "email": "muh.amirul.ihsan@gmail.com"
        }
    ],
    "require": {
        "php" : "^5.6|^7.0",
        "illuminate/support": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0"
    },
    "autoload" : {
        "psr-4" : {
            "Eskater\\Fcm\\" : "src/"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eskater\\Fcm\\FcmServiceProvider"
            ],
            "aliases" : {
                "Fcm": "Eskater\\Fcm\\FcmFacade"
            }
        }
    }
}
